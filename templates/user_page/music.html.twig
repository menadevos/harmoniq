<h3>Choisissez votre musique :</h3>

<form method="post" action="{{ path('user_confirm_music') }}">
    <select name="musicType">
        {% for music in musicOptions %}
            <option value="{{ music }}">{{ music }}</option>
        {% endfor %}
    </select>
    <button type="submit">Confirmer</button>
</form>
